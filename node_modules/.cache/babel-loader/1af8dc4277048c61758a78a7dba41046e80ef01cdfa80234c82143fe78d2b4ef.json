{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      task: '',\n      index: null,\n      update: false,\n      tasktoupdate: {},\n      done: false,\n      tasks: [\n        // {title: \"Do homework every day this week\", created_at: \"2024/04/29\", done: false, done_at: \"\"},\n      ]\n    };\n  },\n  methods: {\n    Status(index) {\n      this.tasks[index].done = !this.tasks[index].done;\n    },\n    addtask() {\n      let curentdate = new Date();\n      let created_at = curentdate.getFullYear() + \" / \" + (curentdate.getMonth() + 1) + \" / \" + curentdate.getDay();\n      this.tasks.push({\n        title: this.task,\n        created_at: created_at,\n        done: this.done,\n        done_at: \"\"\n      });\n      if (localStorage.tasks) {\n        localStorage.tasks.push({\n          title: this.task,\n          created_at: created_at,\n          done: this.done,\n          done_at: \"\"\n        });\n      } else {\n        localStorage.setItem('task');\n      }\n    },\n    deleteTask(index) {\n      console.log({\n        index: index,\n        task: this.task[index]\n      });\n      this.tasks.splice(index, 1);\n    },\n    toupdate(index) {\n      this.update = true;\n      this.tasktoupdate = this.tasks[index];\n    }\n  },\n  computed: {\n    iconeStatus() {\n      return done => {\n        return done ? \"done_all\" : \"watch_check\";\n      };\n    },\n    title() {\n      return done => {\n        return done ? \"mark as not done\" : \"mark as done\";\n      };\n    }\n  }\n};","map":{"version":3,"names":["name","props","msg","String","data","task","index","update","tasktoupdate","done","tasks","methods","Status","addtask","curentdate","Date","created_at","getFullYear","getMonth","getDay","push","title","done_at","localStorage","setItem","deleteTask","console","log","splice","toupdate","computed","iconeStatus"],"sources":["/var/w